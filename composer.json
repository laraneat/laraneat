{
    "name": "laraneat/laraneat",
    "type": "project",
    "description": "The Laraneat framework.",
    "keywords": [
        "laraneat",
        "laravel",
        "php",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-tokenizer": "*",
        "doctrine/dbal": "^3.0.0",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^1.0.6",
        "guzzlehttp/guzzle": "^7.2.0",
        "jackardios/laravel-json-api-paginate": "dev-master",
        "jackardios/laravel-query-wizard": "dev-master",
        "laraneat/modules": "dev-master",
        "laravel/framework": "^8.55",
        "laravel/tinker": "^2.6.0",
        "lorisleiva/laravel-actions": "^2.1",
        "wikimedia/composer-merge-plugin": "^2.0.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5.5",
        "barryvdh/laravel-ide-helper": "^2.9.0",
        "facade/ignition": "^2.5.13",
        "fakerphp/faker": "^1.13.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.3.0",
        "phpunit/phpunit": "^9.5.2",
        "roave/security-advisories": "dev-latest",
        "tutorigo/laravel-ide-macros": "^1.5",
        "wulfheart/laravel-actions-ide-helper": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Ship/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:macros"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "app/Ship/composer.json",
                "app/Modules/*/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": true
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
